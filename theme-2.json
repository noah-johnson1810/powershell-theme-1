{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#ffb954",
          "style": "plain",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "#006c76",
          "foreground": "#1D2021",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": "- root \uf0e7",
          "type": "root"
        },
        {
          "background": "#00a9b9",
          "foreground": "#1D2021",
          "powerline_symbol": "\ue0b0 ",
          "properties": {
            "folder_separator_icon": "/",
            "style": "folder"
          },
          "style": "powerline",
          "template": "\ue5ff {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#00d8eb",
          "foreground": "#1D2021",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        },
        {
          "background": "#5af1ff",
          "background_templates": ["{{ if gt .Code 0 }}#f1184c{{ end }}"],
          "foreground": "#1D2021",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": " \u265E  ",
          "icon_size": 1,
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": false,
      "segments": [
        {
          "foreground": "#ffb954",
          "style": "plain",
          "template": "\u21A3  ",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "type": "spotify",
      "style": "powerline",
      "powerline_symbol": "ùÑû",
      "foreground": "#ffffff",
      "background": "#1BD760",
      "properties": {
        "playing_icon": "‚èµ ",
        "paused_icon": "‚è∏ ",
        "stopped_icon": "‚èπ "
      }
    },
    {
      "type": "executiontime",
      "style": "powerline",
      "powerline_symbol": "ÓÇ∞",
      "foreground": "#ffffff",
      "background": "#8800dd",
      "template": "{{ .FormattedMs }} ",
      "properties": {
        "threshold": 0,
        "style": "austin",
        "always_enabled": true
      }
    }
  ],
  "version": 2
}